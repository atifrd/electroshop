{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ElectroShopFront": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": { "@schematics/angular:component": {"styleext": "scss"}},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ElectroShopFront",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200,
            "host": "localhost",
            "browserTarget": "ElectroShopFront:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ElectroShopFront:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ElectroShopFront:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ElectroShopFrontAdmin": {
      "root": "",
      "sourceRoot": "admin",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ElectroShopFrontAdmin",
            "index": "admin/index.html",
            "main": "admin/main.ts",
            "polyfills": "admin/polyfills.ts",
            "tsConfig": "admin/tsconfig.app.json",
            "assets": [
              "admin/favicon.ico",
              "admin/assets"
            ],
            "styles": [
              "node_modules/normalize.css/normalize.css",
              "node_modules/font-awesome/scss/font-awesome.scss",
              "node_modules/bootstrap/less/bootstrap.less",
              "node_modules/sweetalert2/dist/sweetalert2.css",
              "node_modules/animate.css/animate.css",
              "admin/assets/iconfonts/iconfont.css",
              "admin/app/shared/_styles.scss",
              "admin/styles.scss"        
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/sweetalert2/dist/sweetalert2.js",
              "node_modules/echarts/dist/echarts.min.js",
              "node_modules/echarts/map/js/china.js",
              "node_modules/echarts/dist/extension/bmap.js",
              "admin/app/shared/theme/echarts/dark.js"        
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "admin/environments/environment.ts",
                  "with": "admin/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4300,
            "host": "localhost",
            "browserTarget": "ElectroShopFrontAdmin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ElectroShopFrontAdmin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ElectroShopFrontAdmin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "admin/test.ts",
            "polyfills": "admin/polyfills.ts",
            "tsConfig": "admin/tsconfig.spec.json",
            "karmaConfig": "admin/karma.conf.js",
            "styles": [
              "admin/styles.css"
            ],
            "scripts": [],
            "assets": [
              "admin/favicon.ico",
              "admin/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "admin/tsconfig.app.json",
              "admin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },  
    "ElectroShopFront-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ElectroShopFront:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ElectroShopFront:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ElectroShopFront"
}